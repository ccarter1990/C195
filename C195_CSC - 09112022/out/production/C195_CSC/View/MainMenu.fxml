<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.MainMenu">
   <children>
      <VBox prefHeight="400.0" prefWidth="400.0">
         <children>
            <AnchorPane prefHeight="80.0" prefWidth="400.0" style="-fx-background-color: #DC143C;">
               <children>
                  <Label fx:id="mainMenuLabel" layoutX="90.0" layoutY="15.0" prefHeight="50.0" prefWidth="220.0" style="-fx-background-color: #D3D3D3;" text="Main Menu">
                     <padding>
                        <Insets left="70.0" />
                     </padding>
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
               </children>
            </AnchorPane>
            <AnchorPane prefHeight="320.0" prefWidth="400.0">
               <children>
                  <Button fx:id="customerButton" layoutX="154.0" layoutY="42.0" mnemonicParsing="false" onAction="#moveToCustomerPage" prefWidth="92.0" text="Customers" />
                  <Button fx:id="appointmentButton" layoutX="154.0" layoutY="96.0" mnemonicParsing="false" onAction="#moveToAppointmentsPage" text="Appointments" />
                  <Button fx:id="reportButton" layoutX="155.0" layoutY="167.0" mnemonicParsing="false" onAction="#moveToReportsPage" prefHeight="25.0" prefWidth="92.0" text="Reports" />
                  <Button fx:id="exitProgramButton" layoutX="281.0" layoutY="275.0" mnemonicParsing="false" onAction="#exitProgram" prefHeight="25.0" prefWidth="92.0" text="Exit Program" />
                  <Button fx:id="logOutButton" layoutX="14.0" layoutY="275.0" mnemonicParsing="false" onAction="#moveToLoginScreen" prefHeight="25.0" prefWidth="92.0" text="Log Out" />
               </children></AnchorPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
